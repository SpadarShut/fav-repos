    "@mui/material": "^5.14.20",
    "@tanstack/react-query": "^5.13.4",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "vite-tsconfig-paths": "^4.2.2",
    "zustand": "^4.4.7"
